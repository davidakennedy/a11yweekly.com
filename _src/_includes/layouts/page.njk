{% extends 'layouts/base.njk' %}
{% block content %}
  <section class="page">
    <div class="page-header stack">
      {% set pagetitle = title or renderData.title %}
      <h1 class="title p-name">{{ pagetitle | markdownify }}</h1>
    </div>
    <div class="page-content">
      {{ content | safe }}
    </div>
  </section>
{% endblock %}
